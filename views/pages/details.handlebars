<header>
    <nav>
        <a href="/">Profile</a>
        <a href="/">About Us</a>
        <a href="/">Sign Out</a>
    </nav>
</header>

<main>
    <h1>{{foundName}}</h1>
    {{#with details}}
        <dl>
            <dt>Name</dt>
            <dd>{{name}}</dd>
            <dt>Location</dt>
            <dd>{{location}}</dd>
            <dt>Style</dt>
            <dd>{{style}}</dd>
            <dt>Description</dt>
            <dd>{{description}}</dd>
            <dt>Rating</dt>
            <dd>{{rating}}</dd>
            <dt>Reviews</dt>
        </dl> 
    {{/with}}
</main>