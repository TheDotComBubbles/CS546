<section>
    <h1>{{title}}</h1>
    <article class=person>
        <h2>{{personName}}</h2>
            <dl>
                <dt>ID</dt>
                <dd>{{person.id}}</dd>
                <dt>First Name</dt>
                <dd>{{person.firstName}}</dd>
                <dt>Last Name</dt>
                <dd>{{person.lastName}}</dd> 
                <dt>Address</dt>
                <dd>{{person.address}}</dd> 
                <dt>Zip Code</dt>
                <dd>{{person.zip}}</dd>
                <dt>Phone</dt>
                <dd>{{person.phone}}</dd>
                <dt>Social Security Number</dt>
                <dd>{{person.ssn}}</dd>
            </dl>
    </article>
</section>